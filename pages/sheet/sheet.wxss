/* pages/sheet/sheet.wxss */
.sheets {
  margin-bottom: 140rpx;
  display: flex; 
  flex-flow: row ;
}

.sheet {
  width: 20%;
}

.blank {
  background:red;
  width: 100%;
  height: 50rpx;
}

.back {
  width: 70%;
  height: 100rpx; 
  color: #F7F7F7;
  background: #279DF7;
  border: none;
  border-radius: 20;
  font-size: 36rpx;
  text-align: center;
  vertical-align: middle;
  position:fixed;
  top: 98%;
  left: 85%;
  transform: translate(-100%, -100%);
}