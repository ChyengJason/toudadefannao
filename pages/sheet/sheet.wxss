/* pages/sheet/sheet.wxss */
.tip {
  display: flex;
  width: 100%;
}

.map {
  margin-bottom: 135rpx;
  width: 100%;
}

.item {
  display: inline-block;
  text-align: center;
  width: 20%;
}

.sep {
  display: block;
  width: 100%;
}

.sep-top {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 35rpx;
  color: #000;
  margin-bottom: 30rpx;
  padding-left: 30rpx; 
  background: #F7F7F7;
}

.sep-bottom {
  margin-bottom: 30rpx;
}

.back {
  width: 80%;
  height: 90rpx; 
  color: #F7F7F7;
  background: #279DF7;
  border: none;
  border-radius: 20;
  font-size: 36rpx;
  text-align: center;
  vertical-align: middle;
  position:fixed;
  top: 98%;
  left: 90%;
  transform: translate(-100%, -100%);
}